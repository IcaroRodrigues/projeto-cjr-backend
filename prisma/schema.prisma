generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(uuid())
  username   String   @unique 
  senha      String   
  gender     String   
  email      String   @unique 
  cargo      String   
  admin      Boolean?
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("users")
}
